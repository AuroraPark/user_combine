<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.mml.Mml_PageMapper">
	<select id="selectRowCount" resultType="Integer">
		select count(seq)
		from mml_content
	</select>
	
	<select id="selectPageList" resultType="com.spring.mml.Mml_ListDTO">
		select * from mml_content
		order by seq desc
		limit #{displayRow} offset #{offset}
	
	</select>



</mapper>