package com.spring.mml;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service("mmlService")
public class MmlServiceImpl implements MmlService {
	
	@Autowired
	private SqlSession sqlSession;
	
	@Autowired
	private MmlDAO mmlDAO;
	
	@Override
	public List<Mml_ContentVO> getMmlList(int id){
		MmlDAO mmlDAO = sqlSession.getMapper(MmlDAO.class);
		List<Mml_ContentVO> mmlList = mmlDAO.getMmlList(id);
		
		return mmlList;
	}
	
	
}
